<% include ../partials/head %>
<body style="margin: 0px;">

<% include ../partials/nav %>
<div class="row">

    <div class="col s10 offset-s1 center"><h3>List of Students/Clients</h3></div>

    <div class="col s10 offset-s1" style="overflow-x: scroll;">

        <table id="myTable" class="responsive-table">
            <thead>

            <tr>
                <th>S/No</th>
                <th>
                    <p>
                        <label>
                            <input class="main_checker" type="checkbox" />
                            <span></span>
                        </label>
                    </p>
                </th>
                <th>Reg No</th>
                <th>Full Name</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Date of Birth</th>
                <th>Gender</th>
                <th>Adress</th>
                <th>Local Government</th>
                <th>State</th>
                <th>Country</th>
                <th>Passport</th>
                <th>Academic Qualification</th>
                <th>Computer Literacy</th>
                <th>Additional Information</th>
                <th>Registration Date</th>
                <th>Affirmation</th>
            </tr>
            </thead>

            <tbody>
            <% user_details.forEach(function (details) { counter++; %>
            <tr>
                <td><%=counter%></td>
                <td>
                    <p>
                        <label>
                            <input class="main_checker" value="<%=details.unique_id %>" type="checkbox" />
                            <span></span>
                        </label>
                    </p>
                </td>
                <!--enrolled_courses:enrolled_courses, course_names:course_names, student_names:student_names-->
                <% var strArray=['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']; %>
                <td><%=details.unique_id %></td>
                <td><%=details.sur_name+' '+details.first_name+' '+details.last_name %></td>
                <td><%=details.email %></td>
                <td><%=details.phone %></td>
                <% var d = new Date(details.dob); %>
                <td><%=d.getDate()+' '+strArray[d.getMonth()]+', '+d.getFullYear()%></td>
                <td><%=details.gender %></td>
                <td><%=details.address %></td>
                <td><%=details.local_government %></td>
                <td><%=details.state %></td>
                <td><%=details.country %></td>
                <td><div style="width: 80px;"><img class="responsive-img" src="/images/passport/<%=details.passport %>" alt="<%=details.sur_name+' '+details.first_name+' '+details.last_name %>" /></div> </td>
                <td><%=details.highest_academic_qualification %></td>
                <td><%=details.computer_literacy %></td>
                <td><%=details.additional_information %></td>

                <% var d = new Date(details.date_created); %>
                <td><%=d.getDate()+' '+strArray[d.getMonth()]+', '+d.getFullYear()%></td>
                <td><%=details.affirmation %></td>


            </tr>
            <% }) %>
            </tbody>
        </table>

    </div>

</div>
<% include ../partials/sidenav %>
</body>
<% include ../partials/footer %>
</html>